ENTRY(EntryAsm)
SECTIONS
{
	.low 0x4000 :
	{
		*(.low)
	}
	__KernelStart = 0xC0000000;

	.text __KernelStart + ADDR(.low) + SIZEOF(.low) : AT(LOADADDR(.low) + SIZEOF(.low))
	{
		*(.text)
		*(.rodata)
	}
	
	.data : AT(LOADADDR(.text) + SIZEOF(.text))
	{
		*(.data)
	}

	__InitFsStart = .;
	.initfs : AT(LOADADDR(.data) + SIZEOF(.data))
	{
		*(.initfs)
	}
	__InitFsEnd = .;

	.bss (NOLOAD) :
	{
		*(.bss)
		*(COMMON)
		__KernelEnd = ABSOLUTE(.);
	}
}