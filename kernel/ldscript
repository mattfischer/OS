ENTRY(Entry)
SECTIONS
{
	.entry 0x4000 :
	{
		*(.entry)
	}
	__KernelStart = 0xC0000000;

	.kernelMap __KernelStart (NOLOAD) :
	{
		*(.kernelMap)
	}

	.text __KernelStart + ADDR(.entry) + SIZEOF(.entry) : AT(LOADADDR(.entry) + SIZEOF(.entry))
	{
		*(.text)
		*(.rodata)
	}
	
	.data : AT(LOADADDR(.text) + SIZEOF(.text))
	{
		*(.data)
	}
	
	.bss (NOLOAD) :
	{
		*(.bss)
		*(COMMON)
		__KernelEnd = ABSOLUTE(.);
	}
}